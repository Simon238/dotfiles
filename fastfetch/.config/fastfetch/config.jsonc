{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "padding": {
            "top": 3
        }
    },
    "display": {
        "size": {
            "binaryPrefix": "jedec"
        }
    },
    "modules": [
        {
            "type": "custom",
            "format": "┌────────────────────── Hardware ──────────────────────┐",
            "outputColor": "red"
        },
        {
            "type": "title",
            "key": " PC",
            "keyColor": "green"
        },
        {
            "type": "cpu",
            "key": "│ ├\udb83\udee0 CPU",
            "showPeCoreCount": true,
            "format": "{1} @ {7} [{8}]",
            "temp": true,
            "keyColor": "green"
        },
        {
            "type": "gpu",
            "key": "│ ├󰍛 GPU",
            "format": "{2}",
            "keyColor": "green"
        },
        {
            "type": "memory",
            "key": "│ ├\uefc5 Memory",
            "keyColor": "green"
        },
        {
            "type": "disk",
            "key": "│ ├\udb85\udedf Disk",
            "keyColor": "green"
        },
        {
            "type": "display",
            "key": "└ └\udb80\udf79 Display",
            "keyColor": "green"
        },
        {
            "type": "custom",
            "format": "└──────────────────────────────────────────────────────┘",
            "outputColor": "red"
        },
        {
            "type": "custom",
            "format": "┌────────────────────── Software ──────────────────────┐",
            "outputColor": "red"
        },
        {
            "type": "os",
            "key": "\uf30a OS",
            "keyColor": "yellow"
        },
        {
            "type": "kernel",
            "key": "│ ├ Kernel",
            "keyColor": "yellow"
        },
        {
            "type": "packages",
            "key": "│ ├󰏖 Packages",
            "keyColor": "yellow"
        },
        {
            "type": "shell",
            "key": "│ ├\ue691 Shell",
            "keyColor": "yellow"
        },
        {
            "type": "terminal",
            "key": "│ ├ Terminal",
            "keyColor": "yellow"
        },
        {
            "type": "command",
            "key": "│ ├\ueab0 OS Age",
            "keyColor": "yellow",
            "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days"
        },
        {
            "type": "uptime",
            "key": "└ └\udb81\udd1b Uptime",
            "keyColor": "yellow"
        },
        {
            "type": "de",
            "key": " DE",
            "keyColor": "blue"
        },
        {
            "type": "lm",
            "key": "│ ├ LM",
            "keyColor": "blue"
        },
        {
            "type": "wm",
            "key": "│ ├ WM",
            "keyColor": "blue"
        },
        {
            "type": "wmtheme",
            "key": "└ └󰉼 Theme",
            "keyColor": "blue"
        },
	{
	    "type": "wifi",
	    "key": " Network",
	    "format": "{ssid} ({7}, {13} GHz)",
	    "keyColor": "magenta"
	},
	{
	    "type": "wifi",
	    "key": "│ ├󰍁 Security",
	    "format": "{10}",
	    "keyColor": "magenta"
	},
	{
	    "type": "localip",
	    "key": "│ ├󰌘 Local IP",
	    "format": "{ipv4}",
	    "keyColor": "magenta"
	},
	{
	    "type": "publicip",
	    "key": "│ ├󰒋 Public IP",
	    "format": "{ip}",
	    "keyColor": "magenta"
	},
	{
	    "type": "bluetooth",
	    "key": "└ └ Bluetooth",
	    "format": "{name} ({battery-percentage})",
	    "keyColor": "magenta"
	},
        {
            "type": "custom",
            "format": "└──────────────────────────────────────────────────────┘",
            "outputColor": "red"
        },
        {
            "type": "custom",
            "format": "                 \u001b[30m  \u001b[31m  \u001b[32m  \u001b[33m  \u001b[34m  \u001b[35m  \u001b[36m  \u001b[37m "
        },
    ]
}
